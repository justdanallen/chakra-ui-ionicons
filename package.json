{
  "version": "0.3.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "generate": "ts-node --project scripts/tsconfig.json scripts/generate.ts",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "preinstall": "npx npm-force-resolutions"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^1.2.1",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "chakra-ui-ionicons",
  "author": "Dan Allen",
  "description": "IonIcons (v5) built for Chakra UI",
  "homepage": "https://chakra-ui-ionicons-demo.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/justdanallen/chakra-ui-ionicons"
  },
  "keywords": ["chakra", "ionic icons", "ion icons", "chakra-ui", "chakra-ui icons"],
  "module": "dist/chakra-ui-ionicons.esm.js",
  "size-limit": [
    {
      "path": "dist/chakra-ui-ionicons.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/chakra-ui-ionicons.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@chakra-ui/icon": "^1.0.3",
    "@chakra-ui/react": "*",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "framer-motion": "^3.2.2-rc.1",
    "handlebars": "^4.7.6",
    "husky": "^4.3.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.9.1",
    "svg-parser": "^2.0.4",
    "ts-node": "^9.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "resolutions": {
    "node-forge": ">=0.10.0"
  }
}
